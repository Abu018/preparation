#include <bits/stdc++.h>
using namespace std;

int main() {
    int arr[6]={1,1,0,1,1,1};
    int count=0;
    int result=0;
    for(int i=0;i<6;i++){
        if(arr[i]==1){
            ++count;
        }else{
            count=0;
        }
result=max(result,count);
    }
cout<<result<<endl;
    return 0;
}
